    <update id="updateLineLoadRatioDev1" parameterType="java.util.List">
        <foreach collection="updateList" item="entity" separator=";">
            UPDATE dev1_load_rate_line
            <set>
                load_rate=#{entity.loadRate}
            </set>
            <where>
                id=#{entity.id}
            </where>
        </foreach>
    </update>

